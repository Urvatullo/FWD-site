{
    "extends": "@tsconfig/svelte/tsconfig.json",
    "compilerOptions": {
      "lib": ["ES2020", "DOM"],
      "target": "ES2020",
      "types": ["svelte"],
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true,
      "noEmit": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules/*", "__sapper__/*", "public/*"]
  }
  